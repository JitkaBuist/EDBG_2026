      <output>
        <soap:body use="literal" />
      </output>
      <fault name="SOAPFault">
        <soap:fault use="literal" name="SOAPFault" namespace="" />
      </fault>
    </operation>
  </binding>
  <service name="CKIdentification">
    <port name="CKIdentificationPort" binding="ws:CKIdentificationBinding">
      <soap:address location="http://localhost:9000/b2b/synchroon/ResponderCKIdentificationRespondingActivity" />
    </port>
  </service>

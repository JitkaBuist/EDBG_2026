<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:sawsdl="http://www.w3.org/ns/sawsdl" xmlns:ccma="urn:nedu:edsn:data:meterreadingrejectionacknowledgement:1:standard" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="urn:nedu:edsn:data:meterreadingrejectionacknowledgement:1:standard" version="1p0" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="MeterReadingRejectionAcknowledgementEnvelope" type="ccma:MeterReadingRejectionAcknowledgementEnvelope" />
  <xsd:complexType name="MeterReadingRejectionAcknowledgementEnvelope">
    <xsd:sequence>
